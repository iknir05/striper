<div class="box">
  <%= will_paginate @menus %>
            <div class="box-header row">
              <div class="col-md-7">
                <h3 class="box-title">Menus</h3>
              </div>
              <div class="col-md-3">
                <%= render 'layouts/admin_search_form'%>
              </div>
              <div class="col-md-2">
                <%= link_to 'Add New Menu', new_admin_menu_path, class: "btn btn-block btn-info" %>
               </div>
            </div>
            <!-- /.box-header -->
            <div class="box-body menu-data">
                <%= render partial: 'menu_data', locals: {menus: @menus}%>
            </div>
</div>