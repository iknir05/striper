<div class="box">
            <div class="box-header row">
              <div class="col-md-6">
                <h3 class="box-title">Job Details</h3>
              </div>
            </div>
            <!-- /.box-header -->
            <div class="box-body">
              <table id="example1" class="table table-bordered table-striped">
                <thead>
                <tr>
                  <th>Job Image</th>
                  <th>Job Title</th>
                  <th>Url</th>
                </tr>
                </thead>
                <tbody>
                <tr>
                  <td><%= image_tag @job.job_image.url(:thumb) %></td>
                  <td><%= @job.title %></td>
                  <td><%= @job.url %> </td>
                </tr>
              </table>
              <br>             
              <%= link_to 'Back', admin_jobs_path, class: "btn btn-default" %>
             
            </div>
</div>