<div class="box">
    <%= will_paginate @reviews %>
            <div class="box-header row">
              <div class="col-md-7">
                <h3 class="box-title">Reviews</h3>
              </div>
              <div class="col-md-3">
                <%= render 'layouts/admin_search_form'%>
              </div>
              <div class="col-md-2 ", style= "float: right;">
                <%= link_to 'Add New Review', new_admin_review_path, class: "btn btn-block btn-info" %>
               </div>
            </div>
            <!-- /.box-header -->
            <div class="box-body review-data">
                <%= render partial: 'review_data', locals: {reviews: @reviews}%>
            </div>
</div>