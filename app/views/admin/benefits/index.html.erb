<div class="box">
  <%= will_paginate @benefits %>
            <div class="box-header row">
              <div class="col-md-6">
                <h3 class="box-title">Benefits</h3>
              </div>
              <div class="col-md-3">
                <%= render 'layouts/admin_search_form'%>
              </div>
              <div class="col-md-2 ", style= "float: right;">
              <%= link_to 'Add New Job', new_admin_benefit_path, class: "btn btn-block btn-info" %>
               </div>
            </div>
            <!-- /.box-header -->
            <div class="box-body benefit-data">
                <%= render partial: 'benefit_data', locals: {benefits: @benefits}%>
            </div>
</div>