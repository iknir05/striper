<div class="box">
   <%= will_paginate @facilities %>
            <div class="box-header row">
              <div class="col-md-7">
                <h3 class="box-title">Facilities</h3>
              </div>
              <div class="col-md-3">
                <%= render 'layouts/admin_search_form'%>
              </div>
              <div class="col-md-2 ", style= "float: right;">
              <%= link_to 'Add New Facility', new_admin_facility_path, class: "btn btn-block btn-info" %>
               </div>
            </div>
            <!-- /.box-header -->
            <div class="box-body facility-data">
                <%= render partial: 'facility_data', locals: {facilities: @facilities}%>              
            </div>
</div>