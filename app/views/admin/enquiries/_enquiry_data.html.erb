<table id="enquiry-table" class="table table-bordered table-striped">
                <thead>
                <tr>
                  <th>First Name</th>
                  <th>Last Name</th>
                  <th>Email</th>
                  <th>Phone Number</th>
                  <th>Zip Code</th>
                  <th>City</th>
                  <th>State</th>
                  <th>Created On</th>
                </tr>
                </thead>
                <tbody>
                <% enquiries.each do |enquiry| %>
                <tr>
                  <td><%= enquiry.first_name %></td>
                  <td><%= enquiry.last_name %></td>
                  <td><%= enquiry.email %></td>
                  <td><%= enquiry.phone_number %></td>
                  <td><%= enquiry.zip_code %></td>
                  <td><%= enquiry.city %></td>
                  <td><%= enquiry.state %></td>
                  <td><%= format_date(enquiry.created_at) %></td>
                </tr>
                <% end %>
</table>