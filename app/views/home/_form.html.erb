
            <%= form_with model: enquiry, url: {controller:"home",action: "create"},remote: true do |form| %>   
                    <div class="form-header">
                        <h3>Get An Estimate</h3>
                    </div>
                    <div class="ajax-alert">
                    </div>

                    <div class="form-body">
                        
                        <!-- row -->
                        <div class="row">
                            <div class="col-lg-12 col-md-6">
                                <p>
                                    <%= form.label :first_name, class:"sr-only" %>
                                    <%= form.text_field :first_name,:class => "form-control",placeholder:"First Name*"%>
                                    <% if enquiry.errors.any? %>
                                      <div id="error_explanation" >
                                        <ul class="field_with_errors">
                                          <% @enquiry.errors.full_messages_for(:first_name).each do |msg| %>
                                            <li><%= msg %></li>
                                          <% end %>
                                        </ul>
                                      </div>
                                    <% end %>
                                </p>
                            </div>
                            <div class="col-lg-12 col-md-6">
                                <p>
                                    <%= form.label :last_name, class:"sr-only" %>
                                    <%= form.text_field :last_name,:class => "form-control",placeholder:"Last Name*"%>
                                    <% if enquiry.errors.any? %>
                                      <div id="error_explanation" >
                                        <ul class="field_with_errors">
                                          <% @enquiry.errors.full_messages_for(:last_name).each do |msg| %>
                                            <li><%= msg %></li>
                                          <% end %>
                                        </ul>
                                      </div>
                                    <% end %>
                                </p>
                            </div>
                 
                            <div class="col-lg-12 col-md-6">
                                <p>
                                    <%= form.label :email, class:"sr-only" %>
                                    <%= form.text_field :email,:class => "form-control",placeholder:"Email*"%>
                                    <% if enquiry.errors.any? %>
                                      <div id="error_explanation" >
                                        <ul class="field_with_errors">
                                          <% @enquiry.errors.full_messages_for(:email).each do |msg| %>
                                            <li><%= msg %></li>
                                          <% end %>
                                        </ul>
                                      </div>
                                    <% end %>
                                </p>
                            </div>
                            <div class="col-lg-12 col-md-6">
                                <p>
                                    <%= form.label :phone_number, class:"sr-only" %>
                                    <%= form.text_field :phone_number,:class => "form-control",placeholder:"Phone Numbar*"%>
                                    <% if enquiry.errors.any? %>
                                      <div id="error_explanation" >
                                        <ul class="field_with_errors">
                                          <% @enquiry.errors.full_messages_for(:phone_number).each do |msg| %>
                                            <li><%= msg %></li>
                                          <% end %>
                                        </ul>
                                      </div>
                                    <% end %>
                                </p>
                            </div>
							              <div class="col-lg-12 col-md-6">
                                <p>
                                    <%= form.label :zip_code, class:"sr-only" %>
                                    <%= form.text_field :zip_code,:class => "form-control",placeholder:"Zipcode*"%>
